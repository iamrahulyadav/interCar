         android:layout_height="36dp"
         android:layout_marginBottom="5dp"
         android:layout_marginLeft="7dp"
         android:layout_marginRight="7dp"
         android:baselineAligned="false"
         android:orientation="horizontal">

         <RelativeLayout
             android:layout_width="51dp"
             android:layout_height="wrap_content">

             <ImageView
                 android:layout_width="50dip"
                 android:layout_height="fill_parent"
                 android:layout_alignParentEnd="true"
                 android:layout_alignParentRight="true"
                 android:layout_alignParentTop="true"
                 android:scaleType="center"
                 android:tint="@color/colorPrimary"
                 app:srcCompat="@drawable/ic_directions_car_black_40dp"
                 />
         </RelativeLayout>

         <RelativeLayout
             android:layout_width="70dp"
             android:layout_height="wrap_content"
             android:layout_weight="3">

             <Spinner
                 android:id="@+id/spinner2"
                 android:layout_marginTop="0dp"
                 android:layout_marginLeft="5dp"
                 android:layout_marginRight="5dp"
                 android:background="@drawable/spiner_basic"
                 android:layout_width="fill_parent"
                 android:layout_height="45dp" />


            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="7dp"
            android:layout_marginRight="7dp"
            android:layout_marginTop="5dp"
            android:baselineAligned="false"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="51dp"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/image1"
                    android:layout_width="50dip"
                    android:layout_height="fill_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:scaleType="center"
                    android:tint="@color/colorPrimary"
                    android:src="@android:drawable/ic_menu_my_calendar" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_weight="3">

                <EditText
                    android:id="@+id/txtdateReervation"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/cast_intro_overlay_focus_radius"
                    android:background="@drawable/rounded_border_textview"
                    android:paddingLeft="15dp"
                    android:ems="10"
                    android:hint="DD/MM/AAAA"
                    android:inputType="textPersonName" />


            </RelativeLayout>

            <RelativeLayout
                android:layout_width="51dp"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/image"
                    android:tint="@color/colorPrimary"

                    android:layout_width="50dip"
                    android:layout_height="fill_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:scaleType="center"
                    android:src="@android:drawable/ic_menu_recent_history" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="70dp"
