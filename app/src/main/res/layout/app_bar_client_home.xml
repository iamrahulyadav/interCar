                            android:layout_width="match_parent"
                            android:background="@drawable/rounded_border_textview"
                            android:textAlignment="center"
                            android:layout_height="70dp"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:orientation="horizontal"
                    android:baselineAligned="false"
                    android:layout_marginBottom="5dp"
                    android:layout_marginLeft="7dp"
                    android:layout_marginRight="7dp">

                    <RelativeLayout
                        android:layout_width="70dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentTop="true"
                            android:paddingTop="20dp"
                            android:textSize="9dp"
                            android:textStyle="bold"
                            android:text="AEROLÍNEA:"
                            android:textColor="@color/place_autocomplete_prediction_secondary_text" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="70dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">
                        <EditText
                            android:text=""
                            android:id="@+id/txt_airlineCompany"
                            android:layout_width="match_parent"
                            android:background="@drawable/rounded_border_textview"
                            android:textAlignment="center"
                            android:layout_height="70dp"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:orientation="horizontal"
                    android:baselineAligned="false"
                    android:layout_marginBottom="5dp"
                    android:layout_marginLeft="7dp"
                    android:layout_marginRight="7dp">

                    <RelativeLayout
                        android:layout_width="70dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentTop="true"
                            android:paddingTop="20dp"
                            android:textSize="9dp"
                            android:textStyle="bold"
                            android:text="#DE VUELO::"
                            android:textColor="@color/place_autocomplete_prediction_secondary_text" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="70dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">
                        <EditText
                            android:text=""
                            android:id="@+id/txt_flyNumber"
                            android:layout_width="match_parent"
                            android:background="@drawable/rounded_border_textview"
                            android:textAlignment="center"
                            android:layout_height="70dp"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true" />
                    </RelativeLayout>
                </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:baselineAligned="false"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="0">

                        <android.support.v7.widget.AppCompatButton
                            android:id="@+id/btn_requetTravelNow"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:height="60dp"
                            android:textSize="30dp"
                            android:width="390dp"
                            android:text="Solicitar"
                            android:textColor="@color/whithe"
                            app:backgroundTint="@color/colorPrimary" />

                    </RelativeLayout>


                </LinearLayout>

            </LinearLayout>


            </ScrollView>


        </android.support.v7.widget.CardView>

    <LinearLayout
     android:id="@+id/contentInfoReervation"
     android:layout_width="match_parent"
     android:layout_height="239dp"
     android:layout_marginTop="50dp"
     android:orientation="vertical"
     android:paddingTop="5dp"
        android:visibility="invisible"
        android:background="@color/blanco"
        >

     <LinearLayout
         android:layout_width="match_parent"
         android:layout_height="36dp"
         android:layout_marginTop="5dp"
         android:layout_marginBottom="5dp"
         android:layout_marginLeft="7dp"
         android:layout_marginRight="7dp"
         android:baselineAligned="false"
         android:orientation="horizontal">

         <RelativeLayout
             android:layout_width="51dp"
             android:layout_height="wrap_content">

             <ImageView
                 android:layout_width="50dip"
                 android:layout_height="fill_parent"
                 android:layout_alignParentEnd="true"
                 android:layout_alignParentRight="true"
                 android:layout_alignParentTop="true"
                 android:scaleType="center"
                 app:srcCompat="@drawable/ic_place_black_24dp" />
         </RelativeLayout>

         <RelativeLayout
             android:layout_width="70dp"
             android:layout_height="wrap_content"
             android:layout_weight="3">




             <AutoCompleteTextView
                 android:text=""
                 android:background="@drawable/rounded_border_textview"
                 android:paddingLeft="15dp"
                 android:id="@+id/autoCompleteTextView"
                 android:layout_width="match_parent"
                 android:layout_height="44dp"
                 android:layout_alignParentBottom="true"
                 android:layout_alignParentEnd="true"
                 android:layout_alignParentLeft="true"
                 android:layout_alignParentRight="true"
                 android:layout_alignParentStart="true"
                 android:layout_alignParentTop="true"
                 android:ems="10"
                 android:hint="Origen del Viaje"
                 android:textColor="@color/place_autocomplete_prediction_primary_text">


             </AutoCompleteTextView>
         </RelativeLayout>
     </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="36dp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="7dp"
            android:layout_marginRight="7dp"
            android:baselineAligned="false"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="51dp"
                android:layout_height="wrap_content">

                <ImageView
                    android:layout_width="50dip"
                    android:layout_height="fill_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:scaleType="center"
                    app:srcCompat="@drawable/ic_directions_primari_24dp" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_weight="3">

                <AutoCompleteTextView

                    android:id="@+id/autoCompleteTextView2"
                    android:layout_width="match_parent"
                    android:layout_height="44dp"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:ems="10"
                    android:hint="Destino del Viaje"
                    android:background="@drawable/rounded_border_textview"
                    android:paddingLeft="15dp">


                </AutoCompleteTextView>
            </RelativeLayout>
        </LinearLayout>

